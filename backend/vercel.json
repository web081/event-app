{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "src": "/uploads/(.*)", "dest": "/index.js" },
    { "src": "/insertImage/(.*)", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
